        "saml": {
          "enabled": true,
          "app_id": "123456789012",
          "url": "https://sso.example.com/saml2/init",
          "username": "user@organization2.com",
          "provider": "SSOProvider",
          "mfa": "Auto",
          "skip_verify": false,
          "timeout": 0,
          "aws_urn": "urn:amazon:webservices",
          "aws_session_duration": 43200,
          "aws_profile": "org-sso"
        }
      }
    },
    "personal": {
